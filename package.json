  "dependencies": {
    "body-parser": "^1.20.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0"
  }
}
